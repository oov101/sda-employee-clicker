  "scripts": {
    "postinstall": "concurrently \"npm:copy-requirejs\" \"atlas\" \"tsc\" \"node-sass src/styles/sass/ -o src/styles/css/\"",
    "copy-requirejs": "copyfiles --flat node_modules/requirejs/require.js src/scripts/libs",
